{
  "name": "adapt-authoring",
  "version": "0.11.0",
  "description": "A server-based user interface for authoring eLearning courses using the Adapt Framework.",
  "homepage": "https://github.com/adapt-security/adapt-authoring",
  "license": "GPL-3.0",
  "bin": {
    "adaptat": "./bin/cli.js"
  },
  "scripts": {
    "start": "node ./bin/cli start"
  },
  "repository": "adapt-security/adapt-authoring",
  "adapt_authoring": {
    "documentation": {
      "enable": true
    },
    "framework": "5"
  },
  "dependencies": {
    "adapt-authoring-auth": "taylortom/adapt-authoring-auth",
    "adapt-authoring-config": "adapt-security/adapt-authoring-config",
    "adapt-authoring-core": "adapt-security/adapt-authoring-core",
    "adapt-authoring-jsonschema": "adapt-security/adapt-authoring-jsonschema",
    "adapt-authoring-lang": "adapt-security/adapt-authoring-lang",
    "adapt-authoring-logger": "adapt-security/adapt-authoring-logger",
    "adapt-authoring-middleware": "taylortom/adapt-authoring-middleware",
    "adapt-authoring-server": "adapt-security/adapt-authoring-server"
  },
  "engines": {
    "node": "12.x"
  }
}
