{
  "name": "adapt-authoring",
  "version": "0.11.0",
  "description": "A server-based user interface for authoring eLearning courses using the Adapt Framework.",
  "homepage": "https://github.com/adapt-security/adapt-authoring",
  "license": "GPL-3.0",
  "bin": {
    "adaptat": "./bin/cli.js"
  },
  "scripts": {
    "start": "node ./bin/cli start"
  },
  "repository": "adapt-security/adapt-authoring",
  "adapt_authoring": {
    "documentation": {
      "enable": true
    },
    "framework": "5"
  },
  "dependencies": {
    "adapt-authoring-auth": "taylortom/adapt-authoring-auth",
    "adapt-authoring-config": "git+ssh://github.com/adapt-security/adapt-authoring-config.git",
    "adapt-authoring-core": "git+ssh://github.com/adapt-security/adapt-authoring-core.git",
    "adapt-authoring-jsonschema": "git+ssh://github.com/adapt-security/adapt-authoring-jsonschema.git",
    "adapt-authoring-lang": "git+ssh://github.com/adapt-security/adapt-authoring-lang.git",
    "adapt-authoring-logger": "git+ssh://github.com/adapt-security/adapt-authoring-logger.git",
    "adapt-authoring-middleware": "taylortom/adapt-authoring-middleware",
    "adapt-authoring-server": "git+ssh://github.com/adapt-security/adapt-authoring-server.git"
  },
  "engines": {
    "node": "12.x"
  }
}
